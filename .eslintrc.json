{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "plugins": ["prettier","react"],
  "rules": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended",
      "google",
      "prettier"
    ],
    "semi": "off",
    "quotes": "off",
    "object-curly-spacing": "off",
    "arrow-parents": "off",
    "linebreak-style": "off",
    "require-jsdoc": [0],
    "arrow-parens": "off",
    "operator-linebreak": "off",
    "no-debugger": "off",
    "indent": "off",
    "space-before-function-paren": "off"
  }
}
